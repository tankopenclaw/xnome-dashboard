{
  "name": "xnome-dashboard",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "worker"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:worker",
    "dev:frontend": "npm --workspace frontend run dev",
    "dev:worker": "npm --workspace worker run dev",
    "build": "npm-run-all build:frontend build:worker",
    "build:frontend": "npm --workspace frontend run build",
    "build:worker": "npm --workspace worker run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
